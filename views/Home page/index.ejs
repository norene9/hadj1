  <!-- ======= Contact Section ======= -->
  <section id="contact" class="contact">
      <div class="container">
          <div class="section-title">
              <h2 style = " font-size: 50px; font-family:'Baloo Chettan 2', cursive;  color: #3e3e3e; line-height: 35px; font-weight: bold; text-align:center;">Pour nous contacter</h2>

          </div>

          <div class="row" data-aos="fade-in">
              <div class="col-lg-5 d-flex align-items-stretch">
                  <div class="info">
                      <div class="address">
                          <i class="fas fa-map-marker-alt"></i>
                          <h4>Location:</h4>
                          <p>École supérieure en informatique de Sidi Bel Abbès</p>
                      </div>

                      <div class="email">
                          <i class="fas fa-envelope-open"></i>
                          <h4>Email:</h4>
                          <p>etudaiant3@esi-sba.dz</p>
                      </div>

                      <div class="phone">
                          <i class="fas fa-phone-volume"></i>
                          <h4>Call:</h4>
                          <p>+213 xxxxxx</p>
                      </div>

                      <iframe
                          src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3259.8674539860676!2d-0.6353639853632892!3d35.20977038030695!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xd7f00290df8ed91%3A0x91bd238762e84ce4!2s%C3%89cole%20sup%C3%A9rieure%20en%20informatique%20de%20Sidi%20Bel%20Abb%C3%A8s!5e0!3m2!1sfr!2sdz!4v1587398772065!5m2!1sfr!2sdz"
                          frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"
                          style="border:0; width: 100%; height: 280px;"></iframe>
                  </div>
              </div>

              <div class="col-lg-7 mt-5 mt-lg-0 d-flex align-items-stretch">
                  <form method="post" role="form" class="php-email-form" action="http://localhost:8080/contact">
                      <div class="form-row">
                          <div class="form-group col-md-6">
                              <label for="name">Your Name</label>
                              <input type="text" name="name" class="form-control" id="name" data-rule="minlen:4"
                                  data-msg="Please enter at least 4 chars" />
                              <div class="validate"></div>
                          </div>

                      </div>
                      <div class="emaildiv">
                          <div class="form-group col-md-6">
                              <label class="emaillabel" for="name">Your Email</label>
                              <input type="email" class="form-control inputlabel" name="email" id="email"
                                  data-rule="email" data-msg="Please enter a valid email" />
                              <div class="validate"></div>
                          </div>
                      </div>


                      <div class="form-group">
                          <label for="name">Subject</label>
                          <input type="text" class="form-control" name="subject" id="subject" data-rule="minlen:4"
                              data-msg="Please enter at least 8 chars of subject" />
                          <div class="validate"></div>
                      </div>
                      <div class="form-group">
                          <label for="name">Message</label>
                          <textarea class="form-control" name="message" rows="10" data-rule="required"
                              data-msg="Please write something for us"></textarea>
                          <div class="validate"></div>
                      </div>
                      <div class="mb-3">
                          <div class="loading">Loading</div>
                          <div class="error-message"></div>
                          <div class="sent-message">
                              Your message has been sent. Thank you!
                          </div>
                      </div>
                      <div class="text-center">
                          <button type="submit">Send Message</button>
                      </div>
                  </form>
              </div>
          </div>
      </div>
  </section>
  <!-- End Contact Section -->

